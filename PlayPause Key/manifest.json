{
  "manifest_version": 2,
  "name": "Play/Pause Key",
  "version": "1.0.1",

  "description": "嘗試將鍵盤上的按鍵對應到網頁上的動作",

  "author": "Wei-Yu Chen",

  "content_scripts": [
    {
      "matches": ["https://reader.readmoo.com/reader/index.html"],
      "all_frames": true,
      "js": [
        "js/lib/lib.js",
        "js/sites/readmoo-reader.js"
      ]
    },
    {
      "matches": ["https://www.youtube.com/watch*"],
      "js": [
        "js/lib/lib.js",
        "js/sites/youtube.js"
      ]
    }
  ]

}